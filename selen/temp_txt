    <script type="text/javascript">
        $(function () {

            $("#FrontPG-1").click(function () {
                HideShow(1);
            });
            $("#FrontPG-2").click(function () {
                HideShow(2);
            });
            $("#FrontPG-3").click(function () {
                HideShow(3);
            });
            $("#FrontPG-4").click(function () {
                HideShow(4);
            });
            $("#FrontPG-5").click(function () {
                HideShow(5);
            });
        });

        function HideShow(value) {
            for (var i = 0; i < 6; i++) {
                if (value == i) {
                    $("#FrontPGT-" + i).removeClass("hide");
                    $("#FrontPGTNumber-" + i).addClass("FrontProdGroupsTitleNumber-mark");
                } else {
                    $("#FrontPGT-" + i).addClass("hide");
                    $("#FrontPGTNumber-" + i).removeClass("FrontProdGroupsTitleNumber-mark");
                }
            }
        }
    </script>

